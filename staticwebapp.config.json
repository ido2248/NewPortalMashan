{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": []
  },
  "routes": [
      {
          "routeId": "home",
          "routeType": "Rewrite",
          "routePattern": "^/$",
          "destination": "/index.html"
      },
      {
          "routeId": "notFound",
          "routeType": "Rewrite",
          "routePattern": ".*",
          "destination": "/index.html"
      }
  ]
}